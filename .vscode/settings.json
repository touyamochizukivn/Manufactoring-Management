{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/__init__.py": true,
        "**/__pycache__": true,
        "**/asgi.py": true,
        "**/wsgi.py": true,
        "**/manage.py": true,
        "**/tests.py": true,
        "**/apps.py": true,
        "**/admin.py": true,
        "**/migrations": true
    },
    "hide-files.files": [],
    "editor.tabSize": 4,
    "[django-html]": {
        "editor.tabSize": 2
    },
    "[html]": {
        "editor.tabSize": 2
    },
    // "editor.insertSpaces": true
}